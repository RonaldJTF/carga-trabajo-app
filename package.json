    "@types/crypto-js": "^4.1.2",
    "@types/jasmine": "~4.3.0",
    "@types/node": "^20.12.7",
    "@types/uuid": "^10.0.0",
    "jasmine-core": "~4.6.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "svgo": "^3.3.2",
    "typescript": "~5.1.3"
  }
